remove_dup <- function(tse) {
tse1 <- tse %>%
mutate(dup1 = duplicated(nr_cpf_candidato),
dup2 = duplicated(nr_cpf_candidato, fromLast=T),
dup_end = ifelse(dup1, T,
ifelse(dup2, T, F))) %>%
filter(!dup_end)
print(paste("foram removidas", nrow(tse) - nrow(tse1)))
return(tse1)
}
